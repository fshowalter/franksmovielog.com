---
import { WatchlistProgress } from "~/components/WatchlistProgress";
import { getProps } from "~/components/WatchlistProgress/getProps";
import Layout from "~/layouts/Layout.astro";

const props = await getProps();
---

<Layout
  canonical={true}
  meta={{ description: props.metaDescription }}
  openGraph={{ description: props.deck, type: "website" }}
  title="Watchlist Progress"
>
  <WatchlistProgress {...props} />
</Layout>
