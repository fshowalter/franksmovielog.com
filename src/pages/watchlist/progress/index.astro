---
import AstroPageShell from "~/astro/AstroPageShell.astro";
import { getProps } from "~/features/watchlist-progress/getProps";
import { WatchlistProgress } from "~/features/watchlist-progress/WatchlistProgress";

const props = await getProps();
---

<AstroPageShell
  canonical={true}
  meta={{ description: props.metaDescription }}
  openGraph={{ description: props.deck, type: "website" }}
  title="Watchlist Progress"
>
  <WatchlistProgress {...props} />
</AstroPageShell>
