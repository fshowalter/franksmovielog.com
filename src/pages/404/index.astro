---
import AstroPageShell from "~/astro/AstroPageShell.astro";
import { Article } from "~/components/article/Article";
import { getProps } from "~/components/article/getProps";

const slug = "not-found";

const deck = '"Dick Laurent is dead."';

const props = await getProps({
  deck,
  slug,
});
---

<AstroPageShell
  canonical={false}
  meta={{ description: deck }}
  openGraph={false}
  title="404: Not Found"
>
  <Article {...props} />
</AstroPageShell>
