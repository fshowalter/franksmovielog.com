---
import { getProps } from "src/components/Underseen/getProps";
import { Underseen } from "src/components/Underseen/Underseen";
import Layout from "src/layouts/Layout.astro";

const props = await getProps();
---

<Layout
  title="Underseen Gems"
  description="Four and five star movies with a below average number of votes on the IMDb."
  image="/reviews/underseen/og.jpg"
>
  <Underseen client:load {...props} />
</Layout>
