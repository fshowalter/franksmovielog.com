---
import { getPropsForUnderseen as getProps } from "~/components/Reviews/getProps";
import { Underseen } from "~/components/Reviews/Underseen";
import Layout from "~/layouts/Layout.astro";

const props = await getProps();
---

<Layout
  canonical={true}
  meta={{ description: props.metaDescription }}
  openGraph={{ description: props.deck, type: "website" }}
  title="Underseen Gems"
>
  <Underseen client:load {...props} />
</Layout>
