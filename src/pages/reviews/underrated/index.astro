---
import { getProps } from "~/components/Underrated/getProps";
import { Underrated } from "~/components/Underrated/Underrated";
import Layout from "~/layouts/Layout.astro";

const props = await getProps();
---

<Layout
  canonical={true}
  meta={{ description: props.metaDescription }}
  openGraph={{ description: props.deck, type: "website" }}
  title="Underrated Surprises"
>
  <Underrated client:load {...props} />
</Layout>
