---
import { getPropsForOverrated as getProps } from "~/components/Reviews/getProps";
import { Overrated } from "~/components/Reviews/Overrated";
import Layout from "~/layouts/Layout.astro";

const props = await getProps();
---

<Layout
  canonical={true}
  meta={{
    description: props.metaDescription,
  }}
  openGraph={{
    description: props.deck,
    type: "website",
  }}
  title="Overrated Disappointments"
>
  <Overrated client:load {...props} />
</Layout>
