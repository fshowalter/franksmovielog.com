---
import { getProps } from "~/components/Viewings/getProps";
import { Viewings } from "~/components/Viewings/Viewings";
import Layout from "~/layouts/Layout.astro";

const props = await getProps();
---

<Layout
  canonical={true}
  meta={{ description: props.metaDescription }}
  openGraph={{ description: props.deck, type: "website" }}
  title="Viewing Log"
>
  <Viewings client:load {...props} />
</Layout>
