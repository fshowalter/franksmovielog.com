---
import { CastAndCrew } from "~/components/CastAndCrew/CastAndCrew";
import { getProps } from "~/components/CastAndCrew/getProps";
import Layout from "~/layouts/Layout.astro";

const props = await getProps();
---

<Layout
  article={false}
  canonical={true}
  description={props.deck}
  title="Cast and Crew"
>
  <CastAndCrew client:load {...props} />
</Layout>
