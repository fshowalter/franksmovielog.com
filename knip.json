{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/pages/**/*.astro",
    "src/astro/search.ts",
    "src/astro/navDrawer.ts",
    "eslint.html.config.js",
    "src/pages/**/*.spec.ts"
  ],
  "project": [
    "**/*",
    "!src/**/__mocks__/**",
    "!content/**",
    "!public/**",
    "!**/__snapshots__/**",
    "!**/__image_snapshots__/**",
    "!src/**/fixtures/**",
    "!scripts/**"
  ],
  "ignoreDependencies": [
    "@anthropic-ai/claude-code",
    "@prettier/plugin-xml",
    "@vitest/coverage-v8",
    "babel-plugin-react-compiler"
  ]
}
