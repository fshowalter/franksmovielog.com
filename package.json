{
  "name": "franksmovielog-com",
  "type": "module",
  "version": "10.8.2",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "build:profile": "DEBUG_PERF=true DEBUG_CACHE=true astro build 2>&1 | tee build-perf.log",
    "check": "astro check",
    "dev": "astro dev",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "knip": "knip",
    "lint": "eslint  .",
    "lint:html": "eslint --config eslint.html.config.js src/**/*.html",
    "lint:fix": "eslint --fix .",
    "lint:spelling": "cspell . --no-progress --show-context --show-suggestions",
    "lint:spelling:fix": "rm -rf project-words.txt && echo \"# Project Words - DO NOT TOUCH - use npm rum lint:spelling:fix instead.\" >> project-words.txt && npm run -s lint:spelling -- --words-only --unique --no-exit-code --no-summary \"**\" | LC_ALL=en_US.UTF-8 sort --ignore-case >> project-words.txt",
    "preview": "astro preview",
    "start": "astro dev",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix",
    "sync": "./sync.js",
    "test:coverage": "TEST_COVERAGE=true vitest run src --coverage.all true --coverage",
    "test:eslint-rule": "node eslint-rules/separate-type-imports.test.js",
    "test:update": "vitest -u run src",
    "test:watch": "vitest src",
    "test": "vitest run src"
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "@astrojs/react": "4.4.2",
    "@astrojs/rss": "4.0.13",
    "@astrojs/sitemap": "3.6.0",
    "@tailwindcss/vite": "4.1.17",
    "astro": "5.15.5",
    "astro-compressor": "1.2.0",
    "gray-matter": "4.0.3",
    "p-limit": "7.2.0",
    "pagefind": "1.4.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "rehype-raw": "7.0.0",
    "rehype-stringify": "10.0.1",
    "remark": "15.0.1",
    "remark-gfm": "4.0.1",
    "remark-rehype": "11.1.2",
    "remark-smartypants": "3.0.2",
    "satori": "0.18.0",
    "sharp": "0.34.5",
    "strip-markdown": "6.0.0",
    "tailwindcss": "4.1.17",
    "unist-util-visit": "5.0.0",
    "vite": "6.4.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "2.0.36",
    "@eslint/js": "9.39.1",
    "@html-eslint/eslint-plugin": "0.48.0",
    "@prettier/plugin-xml": "3.4.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/estree": "1.0.8",
    "@types/hast": "3.0.4",
    "@types/jsdom": "27.0.0",
    "@types/mdast": "4.0.4",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/unist": "3.0.3",
    "@vitest/coverage-istanbul": "3.2.4",
    "@vitest/eslint-plugin": "1.4.2",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "chokidar": "4.0.3",
    "cspell": "9.3.0",
    "eslint": "9.39.1",
    "eslint-plugin-astro": "1.5.0",
    "eslint-plugin-better-tailwindcss": "3.7.10",
    "eslint-plugin-perfectionist": "4.15.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-unicorn": "62.0.0",
    "globals": "16.5.0",
    "jsdom": "27.1.0",
    "knip": "5.68.0",
    "memfs": "4.50.0",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "sirv": "3.0.2",
    "stylelint": "16.25.0",
    "stylelint-config-standard": "39.0.1",
    "stylelint-order": "7.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.4",
    "unionfs": "4.6.0",
    "vitest": "3.2.4",
